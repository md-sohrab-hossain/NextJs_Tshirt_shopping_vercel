{
  "name": "teshirt_design",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.(ts|tsx)": [
      "eslint --color",
      "prettier --check"
    ],
    "src/**/*.scss": [
      "stylelint",
      "prettier --check"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "test:e2e": "cross-env NODE_ENV=test jest --clearCache && jest -c jest.config.e2e.js --detectOpenHandles --forceExit",
    "test:e2e:watch": "cross-env NODE_ENV=test HEADLESS=false jest -c jest.config.e2e.js --watch",
    "test:unit": "cross-env NODE_ENV=test jest -c jest.config.unit.js",
    "gen:component": "hygen component new",
    "lint": "run-s lint:type lint:js prettier-write prettier-write:js prettier-write:style",
    "lint:js": "eslint ./src --ext .js --ext .jsx --fix",
    "lint:style": "stylelint \"./src/**/*.{css,scss}\"",
    "lint:type": "tsc -p ./jsconfig.json",
    "prettier-check": "prettier --check \"src/**/*.@(js|jsx|scss)\"",
    "prettier-check:script": "prettier --check \"src/**/*.@(js|jsx)\"",
    "prettier-check:style": "prettier --check \"src/**/*.@(scss)\"",
    "prettier-write": "prettier --write \"src/**/*.@(js|jsx|scss)\"",
    "prettier-write:js": "prettier --write \"*.@(js|jsx|scss)\"",
    "prettier-write:script": "prettier --write \"src/**/*.@(js|jsx)\"",
    "prettier-write:style": "prettier --write \"src/**/*.@(scss)\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@stripe/stripe-js": "^1.16.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.26.0",
    "critters": "0.0.10",
    "micro": "^9.3.4",
    "mongodb-client-encryption": "^1.2.5",
    "mongoose": "^5.12.14",
    "next": "^12.1.4",
    "next-absolute-url": "^1.2.2",
    "next-auth": "^3.27.0",
    "next-connect": "^0.10.1",
    "next-redux-wrapper": "^6.0.2",
    "nodemailer": "^6.6.2",
    "puppeteer": "^13.5.2",
    "react": "17.0.2",
    "react-bootstrap": "^1.6.1",
    "react-dom": "17.0.2",
    "react-js-pagination": "^3.0.2",
    "react-redux": "^7.2.4",
    "react-toastify": "^7.0.4",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0",
    "sass": "^1.35.1",
    "stripe": "^8.165.0",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@fourdigit/stylelint-config-fourdigit": "^2.0.0",
    "@storybook/addon-actions": "^6.4.21",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-interactions": "^6.4.21",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/builder-webpack5": "^6.4.21",
    "@storybook/manager-webpack5": "^6.4.21",
    "@storybook/react": "^6.4.21",
    "@storybook/testing-library": "^0.0.9",
    "@whitespace/storybook-addon-html": "^5.0.0",
    "babel-loader": "^8.2.4",
    "change-case": "^4.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-storybook": "^0.5.8",
    "hygen": "^6.2.0",
    "jest": "^27.3.1",
    "jest-enzyme": "^4.2.0",
    "jest-puppeteer": "^6.0.0",
    "node-sass-glob-importer": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.6.2",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-scss": "^4.2.0"
  }
}